{
    "scale": 2,
    "vertical_spacing": 1.4,
    "microprint_width": 140,
    "default_colors": {
        "background_color": "rgb(30, 30, 30)",
        "text_color": "white"
    },
    "line_rules": {
        "(?:^|\\W)FAIL(?:$|\\W)": {
            "text_color": "white",
            "background_color": "red"
        },
        "(?:^|\\W)warning(?:$|\\W)": {
            "text_color": "black",
            "background_color": "yellow"
        }
    },
    "font-family": "monospace, monospace"
}
